
// Lesson 2
public class Lesson2 {

    public static void main(String[] args) {

  System.out.print("The number of keys on a keyboard is ");
  System.out.println("102");

  System.out.print("The value of PI is: ");
  double pi = 3.14;
  System.out.println(pi);

  double circ;
  double rad = 3.0;
  // the radius
  // Math.PI is Java's stored constant for Ï€
  circ = 2 * Math.PI * rad;
  System.out.print("The circumference of a circle of radius 3.0 is ");
  System.out.println(circ);
}  
}



